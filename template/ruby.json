{
    "for": {
        "head": "for $1 do",
        "tail": "end"
    },
    "forin": {
        "head": "for( $1 in $2 ){",
        "tail": "}"
    },
    "while": {
        "head": "while $1 do",
        "tail": "end"
    },
    "if": {
        "head": "if $1 then",
        "tail": "end"
    },
    "else": {
        "head": "else( $1 ){",
        "tail": "}"
    },
    "elseif": {
        "head": "elseif( $1 ){",
        "tail": "}"
    },
    "switch": {
        "head": "case $1",
        "entry": "when $1 ",
        "tail": "end"
    },
    "function": {
        "head": "def $1 $2 ",
        "tail": "end"
    },
    "class": {
        "head": "class $1 \n",
        "tail": "end"
    },
    "method": {
        "head": "def $1 $2",
        "tail": "end"
    },
    "try": {
        "head": "try{",
        "tail": "}"
    },
    "catch": {
        "head": "catch( $1 ){",
        "tail": "}"
    },
    "finally": {
        "head": "finally{",
        "tail": "}"
    },
    "delimiter": {
        "head": "",
        "tail": "end"
    }
}
